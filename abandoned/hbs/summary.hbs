<g transform="translate({{x}}, {{y}})" class="{{alignment}}-summary">
  <text x="0" y="0" font-size="14">
    <use xlink:href="#identifier-label"/>
    {{  identifier }}
  </text>
  <text x="0" y="20" font-size="14">
    <use xlink:href="#created-label"/>
    {{ created }}
  </text>
  <text x="0" y="40" font-size="14">
    <use xlink:href="#modified-label"/>
    {{ modified }}
  </text>
</g>
